{
  "name": "ludum-dare-34",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "npm run build && open dist/index.html && npm run watch",
    "watch": "watchify src/index.js -o dist/bundle.js -dv -t [ babelify --presets [ es2015 ] ]",
    "copy-html": "cp src/index.html dist/",
    "make-dist": "mkdir -p dist",
    "bundle": "browserify src/index.js -o dist/bundle.js -t [ babelify --presets [ es2015 ] ]",
    "bundle-prd": "browserify src/index.js  -t [ babelify --presets [ es2015 ] ] | uglifyjs > dist/bundle.js",
    "clean": "rm -rf dist/*",
    "build": "npm run make-dist && npm run clean && npm run bundle && npm run copy-html",
    "build-prd": "npm run make-dist && npm run clean && npm run bundle-prd && npm run copy-html"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Deadarius/ludum-dare-34.git"
  },
  "author": "Alex Lapa",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Deadarius/ludum-dare-34/issues"
  },
  "homepage": "https://github.com/Deadarius/ludum-dare-34#readme",
  "dependencies": {
    "babylonjs": "^2.2.0",
    "immutable": "^3.7.5",
    "redux": "^3.0.4"
  },
  "devDependencies": {
    "babel-eslint": "^4.1.6",
    "babel-preset-es2015": "^6.3.13",
    "babelify": "^7.2.0",
    "browserify": "^12.0.1",
    "eslint": "^1.10.3",
    "eslint-config-standard": "^4.4.0",
    "eslint-plugin-standard": "^1.3.1",
    "watchify": "^3.6.1"
  }
}
